                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Thu.MaThu, Thu.TenThu, Loai.TenLoai, Thu.SoLuong, Thu.NguonGoc, Thu.DacDiem, SuKien.TenSukien, KhacPhuc.TenCachKhacPhuc, NhanVien.TenNhanVien, Chuong.MaChuong
